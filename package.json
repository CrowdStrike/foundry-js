{
  "name": "@crowdstrike/foundry-js",
  "version": "0.0.0",
  "description": "CrowdStrike Foundry.js SDK",
  "repository": {
    "type": "git",
    "url": "https://github.com/CrowdStrike/foundry-js.git"
  },
  "license": "UNLICENSED",
  "author": "CrowdStrike UX Team",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "directories": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "_build": "concurrently 'npm:build:*'",
    "_start": "concurrently 'npm:watch:*'",
    "build": "rollup -c ./config/rollup.config.ts --configPlugin typescript",
    "lint": "concurrently 'npm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'npm:lint:*:fix' --names 'fix:'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepublishOnly": "yarn build",
    "start": "yarn build:js --watch",
    "test": "vitest"
  },
  "dependencies": {
    "typescript-memoize": "^1.1.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "14.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "11.0.0",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "concurrently": "^8.0.1",
    "eslint": "^8.40.0",
    "happy-dom": "^9.10.9",
    "rollup": "2.79.1",
    "tslib": "2.5.0",
    "typescript": "5.0.2",
    "vitest": "^0.31.0"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
